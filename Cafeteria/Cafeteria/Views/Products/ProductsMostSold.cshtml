@model IEnumerable<Cafeteria.Models.Product>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VecesOrdenado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VecesOrdenado)
        </td>
        <td>
        
        </td>
    </tr>
}

</table>
